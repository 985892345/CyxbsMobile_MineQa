<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="88dp">

    <ImageButton
        android:id="@+id/store_common_toolbar_arrow_left"
        android:layout_width="7dp"
        android:layout_height="16dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="60.5dp"
        android:background="?attr/selectableItemBackgroundBorderless"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/store_ic_arrow_left" />

    <View
        android:id="@+id/store_common_toolbar_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/store_shape_divide_line" />
</androidx.constraintlayout.widget.ConstraintLayout>